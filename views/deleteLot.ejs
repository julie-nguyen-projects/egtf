<%- include("_header") -%>

<h1>Effacer un lot ?</h1>

<form action="/lots/delete/<%= model.Lot_ID %>" method="post">
    <div class="form-horizontal">

        <%- include("_displayLot") -%>

        <div class="form-group row">
            <label class="col-form-label col-sm-2"></label>
            <div class="col-sm-10">
                <input type="submit" value="Effacer" class="btn btn-default btn-danger" />
                <a class="btn btn-outline-dark cancel" href="/lots">Annuler</a>
            </div>
        </div>
    </div>
</form>

<%- include("_footer") -%>
