<div class="form-group row">
    <label class="col-form-label col-sm-2" for="NomBoutique">Nom de la boutique</label>
    <div class="col-sm-8">
        <input autofocus class="form-control" name="NomBoutique" value="<%= model.NomBoutique %>" />
    </div>
</div>

<div class="form-group row">
    <label class="col-form-label col-sm-2" for="Maison">Maison</label>
    <div class="col-sm-7">
        <input class="form-control" name="Maison" value="<%= model.Maison %>" />
    </div>
</div>

<div class="form-group row">
    <label class="col-form-label col-sm-2" for="Description">Description</label>
    <div class="col-sm-10">
        <textarea class="form-control" cols="20" name="Description" maxlength="32000" rows="7"><%= model.Description %></textarea>
    </div>
</div>

<div class="form-group row">
    <label class="col-form-label col-sm-2" for="Livraison">Livraison</label>
    <div class="col-sm-7">
        <input class="form-control" name="Livraison" value="<%= model.Livraison %>" />
    </div>
</div>
